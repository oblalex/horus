<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="ua.cn.stu.oop.horus.core.domain.servercommander.config.ConfigParameter" table="sc_cfg_parameter">
    <id name="id" type="long">
      <column name="id"/>
      <generator class="sequence">
        <param name="sequence">sc_cfg_parameter_seq</param>
      </generator>
    </id>
    <property column="code_name" name="codeName" not-null="true" unique="true" type="string"/>
    <many-to-one cascade="delete" class="ua.cn.stu.oop.horus.core.domain.text.TitleLink" column="title_link_id" name="titleLink" not-null="true"/>
  </class>
</hibernate-mapping>
