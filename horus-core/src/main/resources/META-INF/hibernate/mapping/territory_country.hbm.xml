<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="ua.cn.stu.oop.horus.core.domain.territory.Country" table="territory_country">
    <id name="id" type="long">
      <column name="id"/>
      <generator class="sequence">
        <param name="sequence">
                    territory_country_seq
                </param>
      </generator>
    </id>
    <property column="game_code" name="gameCode" not-null="true" type="string"/>
    <property column="exists" name="exists" not-null="true" type="true_false"/>
    <many-to-one cascade="all" class="ua.cn.stu.oop.horus.core.domain.text.TitleLink" column="title_link_id" name="titleLink" not-null="true" unique="true"/>
    <many-to-one class="ua.cn.stu.oop.horus.core.domain.file.DBFile" column="flag_icon_file_id" name="flagIcon" not-null="true" unique="true"/>
  </class>
</hibernate-mapping>
