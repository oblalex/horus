<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="ua.cn.stu.oop.horus.core.domain.servercommander.ServerCommander" table="server_commander">
    <id name="id" type="long">
      <column name="id"/>
      <generator class="sequence">
        <param name="sequence">
                    server_commander_seq
                </param>
      </generator>
    </id>
    <property column="ready" name="ready" not-null="true" type="true_false"/>
    <property column="connected" name="connected" not-null="true" type="true_false"/>
    <property column="in_training_mode" name="inTrainingMode" not-null="true" type="true_false"/>
    <property column="title" name="title" not-null="true" type="string" unique="true"/>
    <property column="commander_address" name="commanderAddress" not-null="true" type="string"/>
    <many-to-one cascade="all" class="ua.cn.stu.oop.horus.core.domain.servercommander.config.ConfigScheme" column="config_scheme_id" name="configScheme" not-null="true" unique="false"/>
  </class>
</hibernate-mapping>
