<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="ua.cn.stu.oop.horus.core.domain.territory.TheatreOfWar" table="territory_theatre_of_war">
    <id name="id" type="long">
      <column name="id"/>
      <generator class="sequence">
        <param name="sequence">
                    territory_theatre_of_war_seq
                </param>
      </generator>
    </id>
    <property column="date_of_openning" name="dateOfOpenning" not-null="true" type="timestamp"/>
    <property column="image_scale_km_per_100_px" name="imageScaleKmPer100Px" not-null="true" type="float"/>
    <many-to-one class="ua.cn.stu.oop.horus.core.domain.file.DBFile" column="image_file_id" name="image" not-null="true" unique="true"/>
    <many-to-one class="ua.cn.stu.oop.horus.core.domain.text.TitleLink" column="title_link_id" name="titleLink" not-null="true" unique="true"/>
    <many-to-one class="ua.cn.stu.oop.horus.core.domain.territory.Node" column="node_id" name="node" not-null="true" unique="true"/>
  </class>
</hibernate-mapping>
