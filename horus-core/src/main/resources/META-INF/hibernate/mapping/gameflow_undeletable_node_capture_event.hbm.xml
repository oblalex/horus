<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="ua.cn.stu.oop.horus.core.domain.gameflow.UndeletableNodeCaptureEvent" table="gameflow_undeletable_node_capture_event">
    <id name="id" type="long">
      <column name="id"/>
      <generator class="sequence">
        <param name="sequence">gameflow_undeletable_node_capture_event_seq</param>
      </generator>
    </id>
    <property column="capture_real_date" name="captureRealDate" not-null="true" type="timestamp"/>
    <property column="capture_game_date" name="captureGameDate" not-null="true" type="timestamp"/>
    <many-to-one class="ua.cn.stu.oop.horus.core.domain.territory.Node" column="node" name="node" not-null="true"/>
    <many-to-one class="ua.cn.stu.oop.horus.core.domain.Belligerent" column="belligerent_id" name="belligerent" not-null="true"/>
  </class>
</hibernate-mapping>
