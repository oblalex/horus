<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="ua.cn.stu.oop.horus.core.domain.object.CertainObjectType" table="object_certain_type">
    <id name="id" type="long">
      <column name="id"/>
      <generator class="sequence">
        <param name="sequence">
                    object_certain_type_seq
                </param>
      </generator>
    </id>
    <properties name="unique_constraint" unique="true">
      <many-to-one class="ua.cn.stu.oop.horus.core.domain.object.ObjectType" column="type_id" name="type"/>
      <many-to-one class="ua.cn.stu.oop.horus.core.domain.object.GameObject" column="object_id" name="object"/>
    </properties>
  </class>
</hibernate-mapping>
