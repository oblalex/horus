<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="ua.cn.stu.oop.horus.core.domain.object.ObjectUsageInfo" table="object_usage_info">
    <id name="id" type="long">
      <column name="id"/>
      <generator class="sequence">
        <param name="sequence">object_usage_info_seq</param>
      </generator>
    </id>
    <properties name="unique_constraint">
      <many-to-one class="ua.cn.stu.oop.horus.core.domain.object.GameObject" column="object_id" name="object" not-null="true"/>
      <many-to-one class="ua.cn.stu.oop.horus.core.domain.territory.SpacetimeUsageInfo" column="usage_info_id" name="usageInfo" not-null="true"/>
    </properties>
  </class>
</hibernate-mapping>
