<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="ua.cn.stu.oop.horus.core.domain.user.group.air.AirForce" table="user_group_air_force">
    <id name="id" type="long">
      <column name="id"/>
      <generator class="sequence">
        <param name="sequence">user_group_air_force_seq</param>
      </generator>
    </id>
    <properties name="uniqueConstraint" unique="true">
      <many-to-one cascade="all" class="ua.cn.stu.oop.horus.core.domain.territory.Country" column="country_id" name="country" not-null="true"/>
      <many-to-one cascade="all" class="ua.cn.stu.oop.horus.core.domain.Belligerent" column="belligerent_id" name="belligerent" not-null="true"/>
    </properties>
    <many-to-one class="ua.cn.stu.oop.horus.core.domain.user.pilot.rank.Rank" column="initial_pilot_rank_id" name="initialPilotRank"/>
    <many-to-one class="ua.cn.stu.oop.horus.core.domain.user.group.air.AirGroupType" column="initial_air_group_type_id" name="initialAirGroupType"/>
    <many-to-one class="ua.cn.stu.oop.horus.core.domain.user.group.air.AirGroupStatus" column="initial_air_group_status_id" name="initialAirGroupStatus"/>
    <many-to-one cascade="all" class="ua.cn.stu.oop.horus.core.domain.text.TitleLink" column="title_link_id" name="titleLink" not-null="true" unique="true"/>
    <many-to-one class="ua.cn.stu.oop.horus.core.domain.file.DBFile" column="emblem_image_file_id" name="emblemImage" not-null="true" unique="true"/>
    <many-to-one class="ua.cn.stu.oop.horus.core.domain.file.DBFile" column="makrings_image_file_id" name="makringsImage" not-null="true" unique="true"/>
  </class>
</hibernate-mapping>
