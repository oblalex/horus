<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="ua.cn.stu.oop.horus.core.domain.loadout.TechnologyUnitLoadoutLockInfo" table="loadout_technology_unit_loadout_lock_info">
    <id name="id" type="long">
      <column name="id"/>
      <generator class="sequence">
        <param name="sequence">loadout_technology_unit_loadout_lock_info_seq</param>
      </generator>
    </id>
    <properties name="unique_constraint">
      <many-to-one class="ua.cn.stu.oop.horus.core.domain.loadout.TechnologyUnitLoadout" column="loadout_id" name="loadout" not-null="true"/>
      <many-to-one class="ua.cn.stu.oop.horus.core.domain.territory.SpacetimeUsageInfo" column="usage_info_id" name="usageInfo" not-null="true"/>
    </properties>
  </class>
</hibernate-mapping>
