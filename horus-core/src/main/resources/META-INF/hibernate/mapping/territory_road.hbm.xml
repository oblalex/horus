<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="ua.cn.stu.oop.horus.core.domain.territory.Road" table="territory_road">
    <id name="id" type="long">
      <column name="id"/>
      <generator class="sequence">
        <param name="sequence">
                    territory_road_seq
                </param>
      </generator>
    </id>
    <property column="type" name="type">
      <type name="org.hibernate.type.EnumType">
        <param name="enumClass">
                    ua.cn.stu.oop.horus.core.domain.territory.Road$RoadType
                </param>
        <param name="type">
                    12
                </param>
      </type>
    </property>
    <many-to-one cascade="all" class="ua.cn.stu.oop.horus.core.domain.territory.Node" column="self_node_id" name="selfNode" not-null="true" unique="true"/>
    <many-to-one cascade="all" class="ua.cn.stu.oop.horus.core.domain.territory.Node" column="place_one_node_id" name="placeOneNode" not-null="true"/>
    <many-to-one cascade="all" class="ua.cn.stu.oop.horus.core.domain.territory.Node" column="place_two_node_id" name="placeTwoNode" not-null="true"/>
  </class>
</hibernate-mapping>
