<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="ua.cn.stu.oop.horus.core.domain.gameflow.MissionBlock" table="gameflow_mission_block">
    <id name="id" type="long">
      <column name="id"/>
      <generator class="sequence">
        <param name="sequence">gameflow_mission_block_seq</param>
      </generator>
    </id>
    <property column="in_training_mode" name="inTrainingMode" not-null="true" type="true_false"/>
    <property column="milliseconds_duration" name="millisecondsDuration" not-null="true" type="long"/>
    <property column="milliseconds_left" name="millisecondsLeft" not-null="true" type="long"/>
  </class>
</hibernate-mapping>
