<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="ua.cn.stu.oop.horus.core.domain.object.ObjectType" table="object_type">
    <id name="id" type="long">
      <column name="id"/>
      <generator class="sequence">
        <param name="sequence">
                    object_type_seq
                </param>
      </generator>
    </id>
    <property name="prefix">
      <type name="org.hibernate.type.EnumType">
        <param name="enumClass">
                    ua.cn.stu.oop.horus.core.domain.object.ObjectPrefixInMissionFile
                </param>
        <param name="type">
                    12
                </param>
      </type>
    </property>
    <property column="score_for_foe_kill" name="scoreForFoeKill" not-null="true" type="int"/>
    <many-to-one cascade="all" class="ua.cn.stu.oop.horus.core.domain.type.Type" column="type_id" name="type" unique="true"/>
  </class>
</hibernate-mapping>
