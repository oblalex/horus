<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="ua.cn.stu.oop.horus.core.domain.territory.RoadSection" table="territory_road_section">
    <id name="id" type="long">
      <column name="id"/>
      <generator class="sequence">
        <param name="sequence">
                    territory_road_section_seq
                </param>
      </generator>
    </id>
    <property column="body" name="body" not-null="true" type="org.hibernatespatial.GeometryUserType"/>
    <property column="bad" name="bad" not-null="true" type="true_false"/>
    <many-to-one cascade="all" class="ua.cn.stu.oop.horus.core.domain.territory.Node" column="node_id" name="node" not-null="true" unique="true"/>
  </class>
</hibernate-mapping>
