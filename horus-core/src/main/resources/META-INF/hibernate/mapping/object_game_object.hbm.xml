<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="ua.cn.stu.oop.horus.core.domain.object.GameObject" table="object_game_object">
    <id name="id" type="long">
      <column name="id"/>
      <generator class="sequence">
        <param name="sequence">
                    object_seq
                </param>
      </generator>
    </id>
    <property column="length_metres" name="lengthMetres" not-null="true" type="double"/>
    <property column="width_metres" name="widthMetres" not-null="true" type="double"/>
    <property column="height_metres" name="heightMetres" not-null="true" type="double"/>
    <property column="capacity" name="capacity" not-null="true" type="double"/>
    <property column="primary_price" name="primaryPrice" not-null="true" type="double"/>
    <property column="code_in_static_section_of_mission_file" name="codeInStaticSectionOfMissionFile" not-null="true" type="string" unique="true"/>
    <many-to-one cascade="all" class="ua.cn.stu.oop.horus.core.domain.text.TitleLink" column="title_link_id" name="titleLink" not-null="true" unique="true"/>
  </class>
</hibernate-mapping>
