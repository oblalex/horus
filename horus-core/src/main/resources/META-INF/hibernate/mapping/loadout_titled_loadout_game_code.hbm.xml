<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="ua.cn.stu.oop.horus.core.domain.loadout.TitledLoadoutGameCode" table="loadout_titled_loadout_game_code">
    <id name="id" type="long">
      <column name="id"/>
      <generator class="sequence">
        <param name="sequence">
                    loadout_titled_loadout_game_code_seq
                </param>
      </generator>
    </id>
    <property column="game_log_code" name="gameLogCode" not-null="true" type="string" unique="true"/>
    <many-to-one cascade="all" class="ua.cn.stu.oop.horus.core.domain.text.TitleLink" column="title_link_id" name="titleLink" not-null="true" unique="true"/>
  </class>
</hibernate-mapping>
